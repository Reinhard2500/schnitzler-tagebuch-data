    </acdh:SpecResource>
    
    <!-- maps arche props to their values via xpath expressions; be aware the current TEI document needs to be encoded as '$doc' -->
    <acdh:TeiLookUps>
        <acdh:hasTitle type="literal">$item//tei:titleStmt/tei:title[@type="main"]/text()|| " (Schnitzler Tagebuch Digital)"</acdh:hasTitle>
        <acdh:hasCoverageStartDate type="date">$item//tei:titleStmt/tei:title[@type="iso-date"]/text()</acdh:hasCoverageStartDate>
        <acdh:isContinuedBy type="resource">$item/@next</acdh:isContinuedBy>
        <acdh:continues type="resource">$item/@pref</acdh:continues>
        <acdh:isPartOf type="resource">$item/@xml:base</acdh:isPartOf>
        <acdh:hasSource type="resource_many">$item//tei:graphic/@url</acdh:hasSource>
    </acdh:TeiLookUps>
    
    <!-- mapping  tei:person to arche:Person -->
    <acdh:PersonLookUps source="indices/listperson.xml">
        <acdh:hasIdentifier type="resource">$archeutils:persons_url||'/'||data($item/@xml:id)</acdh:hasIdentifier>
        <acdh:hasIdentifier type="resource">$item/tei:idno[@type="GND"]/text()</acdh:hasIdentifier>
        <acdh:hasTitle type="literal">normalize-space($item/tei:persName[1]/tei:forename/text()||' '||$item/tei:persName[1]/tei:surname/text())</acdh:hasTitle>
    </acdh:PersonLookUps>
    
    <!-- mapping  tei:place to arche:Place -->
    <acdh:PlaceLookUps source="indices/listplace.xml">
        <acdh:hasIdentifier type="resource">$archeutils:places_url||'/'||data($item/@xml:id)</acdh:hasIdentifier>
        <acdh:hasIdentifier type="resource">replace(substring($item/tei:idno[@type="geonames"]/text(), 1, string-length($item/tei:idno[@type="geonames"]/text())-1), 'http', 'https')</acdh:hasIdentifier>
        <acdh:hasTitle type="literal">normalize-space($item/tei:placeName[1]/text())</acdh:hasTitle>
    </acdh:PlaceLookUps>
    
    <acdh:Mentions collection="editions">
        <acdh:hasActor type="resource">distinct-values(data($item//tei:body//tei:rs[@type="person" and @ref]/@ref))</acdh:hasActor>
        <acdh:hasSpatialCoverage type="resource">distinct-values(data($item//tei:body//tei:rs[@type="place" and @ref]/@ref))</acdh:hasSpatialCoverage>
    </acdh:Mentions>
</acdh:ACDH>